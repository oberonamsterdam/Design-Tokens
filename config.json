{
    "source": ["tokens/**/*.json"],
    "platforms": {
        "js": {
            "buildPath": "generated",
            "files": [
                {
                    "destination": "tokens.ts",
                    "format": "javascript/es6"
                }
            ],
            "transforms": [
                "attribute/cti",
                "name/cti/pascal",
                "color/hex",
                "remify"
            ]
        }
    }
}
